{
  "name": "nuska",
  "version": "0.1.0",
  "description": "Sever library for TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" \"*.md\"",
    "format:src": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:tests": "prettier --write \"tests/**/*.{ts,tsx}\""
    
  },
  "keywords": [
    "typescript",
    "library",
    "server",
    "REST"
  ],
  "author": "Nicholas Morell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickmorell/server"
  },
  "bugs": {
    "url": "https://github.com/nickmorell/server/issues"
  },
  "homepage": "https://github.com/nickmorell/server#readme",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-ts": "^1.1.22",
    "prettier": "3.7.4",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
